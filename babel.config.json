{
  "presets": [["@babel/preset-env", { "loose": true }], "@babel/preset-react"],
  "plugins": [
    ["@babel/plugin-proposal-class-properties", { "loose": true }],
    "@vanilla-extract/babel-plugin"
  ],
  "env": {
    "test": {
      "presets": [
        "@babel/preset-typescript",
        [
          "@babel/preset-env",
          {
            "targets": {
              "node": "current"
            }
          }
        ]
      ],
      "plugins": ["dynamic-import-node"]
    }
  }
}
