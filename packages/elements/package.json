{
  "name": "@seed-ui/elements",
  "version": "0.8.0",
  "description": "",
  "files": [
    "dist",
    "package.json",
    "CHANGELOG.md"
  ],
  "types": "dist/types/index.d.ts",
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/smrwsky/seed-ui.git",
    "directory": "packages/elements"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "yarn clean && yarn build:types && yarn build:cjs && yarn build:es",
    "build:cjs": "cross-env NODE_ENV=cjs babel src --out-dir dist/cjs  --extensions \".ts,.tsx\" --ignore \"**/*.test.ts\",\"**/*.test.tsx\",\"**/*.docs.mdx\",\"**/*.stories.tsx\",\"**/test-utils.tsx\"",
    "build:es": "cross-env NODE_ENV=es babel src --out-dir dist/es  --extensions \".ts,.tsx\" --ignore \"**/*.test.ts\",\"**/*.test.tsx\",\"**/*.docs.mdx\",\"**/*.stories.tsx\",\"**/test-utils.tsx\"",
    "build:types": "tsc -p tsconfig.json --emitDeclarationOnly",
    "clean": "rimraf dist",
    "format": "yarn format:ts && yarn format:js && yarn format:json",
    "format:js": "prettier --write \"**/*.js\"  --loglevel error",
    "format:json": "prettier --parser json --write \"**/*.{json,prettierrc}\" --loglevel error",
    "format:ts": "prettier --parser typescript --write \"**/*.{ts,tsx}\" --loglevel error",
    "format:yaml": "prettier --parser yaml --write \"**/*.{yml,yaml}\" --loglevel error",
    "lint": "eslint src --ext .ts,.tsx --cache",
    "lint:fix": "yarn lint --fix",
    "lint:ci": "yarn lint --format github",
    "lint:quiet": "yarn lint --quiet",
    "precommit": "lint-staged",
    "test": "jest --config jest.config.js --passWithNoTests",
    "test:watch": "yarn test --watch",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "keywords": [],
  "author": "Sergey Murawsky",
  "license": "MIT",
  "peerDependencies": {
    "@vanilla-extract/css": "^1.6.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/plugin-transform-runtime": "^7.14.3",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.8.3",
    "@testing-library/dom": "^8.15.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.5",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^26.0.24",
    "@types/jest-axe": "^3.5.4",
    "@types/lodash": "^4.14.182",
    "@types/react-dom": "^18.0.3",
    "@types/react-modal": "^3.13.1",
    "@types/react-transition-group": "^4.4.4",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "@vanilla-extract/babel-plugin": "^1.1.1",
    "@vanilla-extract/css": "^1.6.1",
    "babel-jest": "^26.6.3",
    "babel-plugin-lodash": "^3.3.4",
    "cross-env": "^7.0.3",
    "esbuild": "^0.13.3",
    "esbuild-plugin-babel": "^0.2.3",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.2.0",
    "eslint-formatter-github": "^1.0.11",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.5",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "glob": "^7.1.6",
    "jest": "^26.6.3",
    "jest-axe": "^6.0.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.6",
    "typescript": "4.2.2"
  },
  "dependencies": {
    "@floating-ui/react-dom": "^1.0.1",
    "@react-hook/debounce": "^4.0.0",
    "@seed-ui/icons": "^0.5.0",
    "@seed-ui/layout": "^0.7.0",
    "@seed-ui/styles": "^0.7.0",
